  <lead>
    <name>Unknown User</name>
    <user>unknown</user>
    <email>unknown@example.com</email>
    <active>yes</active>
  </lead>

  <date>2015-05-17</date>
  <version>
    <release>0.0.1dev</release>
    <api>0.0.1dev</api>
  </version>
  <stability>
    <release>devel</release>
